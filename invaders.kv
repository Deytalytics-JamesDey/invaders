<InvadersGame>:
    test_invader: test_invader
    test_bullet: test_bullet
    player_ship: player_ship


    canvas:

    Invader:
        id: test_invader
        image: 'images/invader2.jpg'
        center_x: root.center_x
        center_y: root.center_y

    Ship:
        id: player_ship
        center_x: root.center_x
        center_y: 0 + self.height

    Bullet:
        id: test_bullet
        width: 5
        height: 5
        center_x: root.center_x
        center_y: root.center_y * 0.66

        
<Invader>:
    canvas:
        Ellipse:
            pos: self.pos
            size: self.size
            source: self.image

<Ship>:
    canvas:
        Ellipse:
            pos: self.pos
            size: self.size
            source: self.image

<Bullet>:
    canvas:
        Ellipse:
            pos: self.pos
            size: self.size